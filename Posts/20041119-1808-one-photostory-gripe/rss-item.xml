<item>
  <title>One PhotoStory Gripe</title>
  <link>http://devhawk.net/2004/11/19/one-photostory-gripe/</link>
  <pubDate>Fri, 19 Nov 2004 18:08:25 +0000</pubDate>
  <dc:creator xmlns:dc="http://purl.org/dc/elements/1.1/">devhawk</dc:creator>
