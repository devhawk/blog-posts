<item>
  <title>Hacking EXIF w/ Omar's PhotoLibrary</title>
  <link>http://devhawk.net/2004/07/09/hacking-exif-w-omars-photolibrary/</link>
  <pubDate>Fri, 09 Jul 2004 22:50:27 +0000</pubDate>
  <dc:creator xmlns:dc="http://purl.org/dc/elements/1.1/">devhawk</dc:creator>
