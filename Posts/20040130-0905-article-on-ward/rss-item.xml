  <title>Article on Ward</title>
  <link>http://devhawk.net/2004/01/30/article-on-ward/</link>
  <pubDate>Fri, 30 Jan 2004 09:05:13 +0000</pubDate>
  <dc:creator xmlns:dc="http://purl.org/dc/elements/1.1/">devhawk</dc:creator>
