<item>
  <title>Blogging from Office 2003</title>
  <link>http://devhawk.net/2003/03/13/blogging-from-office-2003/</link>
  <pubDate>Thu, 13 Mar 2003 16:43:24 +0000</pubDate>
  <dc:creator xmlns:dc="http://purl.org/dc/elements/1.1/">devhawk</dc:creator>
