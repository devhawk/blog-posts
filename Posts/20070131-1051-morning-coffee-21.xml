  <title>Morning Coffee 21</title>
  <link>http://devhawk.net/2007/01/31/morning-coffee-21/</link>
  <pubDate>Wed, 31 Jan 2007 10:51:22 +0000</pubDate>
  <dc:creator xmlns:dc="http://purl.org/dc/elements/1.1/">devhawk</dc:creator>
